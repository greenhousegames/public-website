{
  "database": {
    "rules": "environments/shared/database.rules.json"
  },
  "hosting": {
    "public": "dist/release",
    "headers": [{
      "source": "/games/**/*.*",
      "headers": [{
      	"key": "Cache-Control",
      	"value": "max-age=0, private, no-store, no-cache, must-revalidate"
      }]
    }],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
