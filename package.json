{
  "name": "@greenhousegames/public-website",
  "version": "1.2.5",
  "scripts": {
    "clean": "(rm -r public || true)",
    "start": "npm run clean && brunch watch --server",
    "build": "npm run clean && brunch build",
    "pretest": "npm run build",
    "test": "npm run test-only",
    "test-only": "karma start",
    "test-debug": "karma start --single-run=false --debug",
    "production": "npm run clean && brunch build --production",
    "predist:debug": "(rm -r dist/debug || true) && mkdir -p dist/debug",
    "dist:debug": "npm run build && gulp finalcopy",
    "predist:release": "(rm -r dist/release || true) && mkdir -p dist/release",
    "dist:release": "npm run production && gulp finalcopy --production",
    "predeploy:debug": "cp environments/debug/firebase.json firebase.json && firebase use debug",
    "deploy:debug": "firebase deploy -m $(npm show ./ version)",
    "postdeploy:debug": "gcloud config set project grenhouse-games-test && gsutil cors set './environments/debug/cors.json' gs://greenhouse-games-test.appspot.com",
    "predeploy:release": "cp environments/release/firebase.json firebase.json && firebase use release",
    "deploy:release": "firebase deploy -m $(npm show ./ version)",
    "postdeploy:release": "gcloud config set project grenhouse-games && gsutil cors set ./environments/release/cors.json gs://greenhouse-games.appspot.com",
    "preversion": "(rm -r dist || true) && mkdir dist",
    "version": "npm run dist:release && git add -A dist",
    "postversion": "git push && git push --tags && npm publish && npm run deploy:release",
    "skipversion:debug": "npm run dist:debug && npm run deploy:debug",
    "skipversion:release": "npm run dist:release && npm run deploy:release"
  },
  "dependencies": {
    "@greenhousegames/bash-dot": "^1.0.0",
    "@greenhousegames/bash-dot-app": "^1.0.0",
    "@greenhousegames/puzzle-share": "^1.0.0",
    "@greenhousegames/puzzle-share-app": "^1.0.0",
    "firebase": "^3.6.1",
    "foundation-sites": "6.3.1",
    "highlightjs": "^9.8.0",
    "jquery": "^2.2.0",
    "motion-ui": "~1.2.2",
    "timeago": "^1.5.3",
    "what-input": "^2.0.0"
  },
  "devDependencies": {
    "after-brunch": "^0.0.5",
    "autoprefixer": "^6.4.1",
    "babel-brunch": "^6.0.6",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.3.13",
    "before-brunch": "^0.0.5",
    "brunch": "^2.8.2",
    "clean-css-brunch": "^2.0.0",
    "css-brunch": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-assetpaths": "^0.3.0",
    "gulp-cli": "^1.2.1",
    "gulp-filter": "^5.0.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^7.1.2",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sitemap": "^4.2.0",
    "gulp-unzip": "^0.2.0",
    "javascript-brunch": "^2.0.0",
    "js-yaml": "^3.4.6",
    "normalize-scss": "^6.0.0",
    "panini": "^1.3.0",
    "postcss-brunch": "^2.0.3",
    "rimraf": "^2.4.3",
    "run-sequence": "^1.2.2",
    "sass-brunch": "^1.8.10",
    "sassy-lists": "^3.0.1",
    "uglify-js-brunch": "^2.0.0",
    "yargs": "^6.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greenhousegames/public-website.git"
  },
  "author": "Greenhouse Games, LLC",
  "license": "",
  "bugs": {
    "url": "https://github.com/greenhousegames/public-website/issues"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "homepage": "https://github.com/greenhousegames/public-website#readme"
}
